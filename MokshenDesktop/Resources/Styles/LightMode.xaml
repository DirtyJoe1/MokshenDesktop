<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">
    <ImageSource x:Key="ThemesBackground">/Resources/Images/themesbackground.png</ImageSource>
    <ImageSource x:Key="ModeImage">/Resources/Images/DarkTheme.png</ImageSource>
    <SolidColorBrush x:Key="Background">#eee3da</SolidColorBrush>
    <SolidColorBrush x:Key="TextColor">Black</SolidColorBrush>
    <SolidColorBrush x:Key="AccentColor">#AC3146</SolidColorBrush>
    <SolidColorBrush x:Key="SecondaryAccentColor">#7e323f</SolidColorBrush>
    <SolidColorBrush x:Key="AlternativeTextColor">White</SolidColorBrush>
    <SolidColorBrush x:Key="AlternativeAccentColor">#263151</SolidColorBrush>
    <ResourceDictionary.MergedDictionaries>
        <materialDesign:CustomColorTheme BaseTheme="Light" PrimaryColor="#AC3146" SecondaryColor="#7e323f" />
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>