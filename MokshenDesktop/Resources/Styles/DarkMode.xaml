<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">
    <ImageSource x:Key="ThemesBackground">/Resources/Images/themesbackground.png</ImageSource>
    <ImageSource x:Key="ModeImage">/Resources/Images/LightTheme.png</ImageSource>
    <SolidColorBrush x:Key="Background">#132047</SolidColorBrush>
    <SolidColorBrush x:Key="TextColor">White</SolidColorBrush>
    <SolidColorBrush x:Key="AccentColor">#263151</SolidColorBrush>
    <SolidColorBrush x:Key="SecondaryAccentColor">#495f9d</SolidColorBrush>
    <ResourceDictionary.MergedDictionaries>
        <materialDesign:CustomColorTheme BaseTheme="Dark" PrimaryColor="#263151" SecondaryColor="#495f9d" />
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>